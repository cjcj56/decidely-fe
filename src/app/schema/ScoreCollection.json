{
    "$schema": "http://www.decidely.com/schemas",
    "$id": "http://www.decidely.com/schemas/ScoreCollection.json",
    "title": "ScoreCollection",
    "type": "object",
    "properties": {
        "scores": {
            "type": "object",
            "propertyNames": { "pattern": "^[0-9]+:[0-9]+$" } 
        },
        "decision": { "type": { "$ref": "Decision.json#/" } }
    },
    "required": ["scores", "decision"],
    "additionalProperties": false
}
